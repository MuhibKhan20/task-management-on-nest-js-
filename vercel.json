{
  "buildCommand": "npm install && npm run vercel-build && node build-frontend.js",
  "outputDirectory": "apps/frontend/dist",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}